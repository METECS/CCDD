<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project cFS_CVS_convert with libraries in sub-folder">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <!--define folder properties-->
    <property name="dir.buildfile" value="."/>
    <property name="dir.workspace" value="${dir.buildfile}/.."/>
    <property name="dir.jarfile" value="${dir.workspace}/tools/cfs_cdd/CFS_TO_CSV_conversion"/>
    <target name="create_run_jar">
        <jar destfile="${dir.jarfile}/cfs_csv_convert.jar">
            <manifest>
                <attribute name="Main-Class" value="cfs_cvs_convert.ConvertCFSStructureToCSV"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="${dir.buildfile}/bin"/>
        </jar>
    </target>
</project>
